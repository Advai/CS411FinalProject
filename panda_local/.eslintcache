 <table style={{ textAlign: 'center', fontSize: '20px' }}>\n                <tr>\n                    <td>\n                        <input class = \"center\" type=\"text\" name=\"p1\" value={this.state.p1} onChange={this.handleInputChange} />\n                    </td>\n                    VS\n                    <td>\n                        <input type=\"text\" name=\"p2\" value={this.state.p2} onChange={this.handleInputChange} />\n                    </td>\n                </tr>\n            </table>\n        </center>\n\n        <button onClick={this.requestHeadtoHead}>Show</button>\n        <br></br>\n            { (\n\n            <div>   \n                <div>\n                <this.ListLoading isLoading={this.state.loading} repos={this.state.repos}/>\n                </div>\n            </div>\n            \n            )}\n\n        </div>\n        )\n    }\n}\n","/Users/apodduturi/Google Drive/UIUC/2020-21 Junior/CS 411 Database Systems/CS411FinalProject/panda_local/src/history.js",[],"/Users/apodduturi/Google Drive/UIUC/2020-21 Junior/CS 411 Database Systems/CS411FinalProject/panda_local/src/components/Navbar.js",["95"],"import React from 'react';\nimport './Navbar.css';\nimport { Navbar, Nav, Form, Button, NavDropdown, FormControl } from 'react-bootstrap';\nimport { withRouter } from 'react-router-dom';\n\nconst Navigation = (props) => {\n    console.log(props);\n    return (\n        <Navbar bg=\"light\" expand=\"lg\">\n        <Navbar.Brand href=\"/\">Panda Local</Navbar.Brand>\n        <Navbar.Toggle />\n        <Navbar.Collapse>\n          <Nav className=\"mr-auto\">\n          <Nav.Link href=\"/Elo\">Rankings</Nav.Link>\n            <Nav.Link href=\"/HeadToHead\">Head to Head</Nav.Link>\n            <Nav.Link href=\"/insert\">Insert Player</Nav.Link>\n            <Nav.Link href=\"/delete\">Delete Player</Nav.Link>\n            <Nav.Link href=\"/update\">Update Player</Nav.Link>\n            {/* <NavDropdown title=\"Games\">\n              <NavDropd